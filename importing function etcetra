import sum_handler
import sub_handler

sum_handler.addition()
sub_handler.sub()



####sum_handler.py
def addition():
    a = int(input("Enter first number "))
    b = int(input("Enter second number "))
    c = a + b
    print(f"Sum is {c}")


###sub_handler.py
def sub():
    a = int(input("Enter first number "))
    b = int(input("Enter second number "))
    c = a - b
    print("Difference is", c)



