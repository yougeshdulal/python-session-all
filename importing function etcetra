def addition():
    a = int(input("Enter first number "))
    b = int(input("Enter second number "))
    c = a + b
    print(f"Sum is {c}")


def sub():
    a = int(input("Enter first number "))
    b = int(input("Enter second number "))
    c = a - b
    print("Difference is",c)


addition()
