list1 = ["ram", 'shyam', 'hari']
print(list1)
list1.append('sita')
list1.insert(2,'nisha')
print(list1)
list1.sort()
print(list1)
list1.sort(reverse=True)
print(list1)
print(len(list1))
print(list1[2])




n = int(input("kati wota naam? "))
list1 = []
for i in range(n):
    name = input("Enter name ")
    list1.append(name)
print(list1)
