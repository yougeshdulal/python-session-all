l1 = []
new_list = []
n = int(input("Kati jana ko detail rakhne? "))
for i in range(n):
    name = input("Enter your name: ")
    age = int(input("Enter your age: "))
    mark = float(input("Enter your mark: "))
    new_list.append(name)
    new_list.append(age)
    new_list.append(mark)
    l1.append(new_list)
print(l1)



l1 = []  #euta blank list banako pachi use garna
# l1 = list()
n = int(input("Kati jana ko detail rakhne? ")) #user sanga element number mageko
let's say user le 2 rakhyo re
n = 2
for i in range(n):
for i in range(2):
    range aba 0 and 1 ma chalcha
    new_list = [] blank narakhda ke huncha?
    narakhda sabai detail lincha refer to the program in the file for same
    i ko value 0 huda:   i = 1
    name = input("Enter your name: ") ram     shyam
    age = int(input("Enter your age: ")) 22    33
    mark = float(input("Enter your mark: ")) 77   88
    new_list ma ram 22 and 77 lai add garyo 
    new_list ma shyam 33 and 88 lai add garyo 
    new_list.append(name) 
    new_list.append(age)
    new_list.append(mark)
    suru ma jaba i = 1 cha, tyo bela aba
    next() ma jaba i = 2 cha, tyo bela aba
    new_list ma ['ram',22, 77] cha
        when i is 2 new_list ma ['shyam',33, 88] cha
    uslai l1 vanne blank list ma add garne aba
    l1.append(new_list)
print(l1)




