# print(l1)
l1 = []
n = int(input("Kati jana ko detail rakhne? "))
for i in range(n):
    name = input("Enter your name: ")
    age = int(input("Enter your age: "))
    mark = float(input("Enter your mark: "))
    l1.append(name)
    l1.append(age)
    l1.append(mark)
print(l1)




