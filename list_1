my_list = ['ram', 'shyam', 'hari', 'sita', 'ram', 'Roshan']
# my_list = list()
# my_list = [] ordered, duplicate allowed, indexed, slicing, mutable
print(my_list)
my_list.sort(reverse=True)
print(my_list)




import random

my_list = ['ram', 'shyam', 'hari', 'sita', 'ram', 'Roshan']
# my_list = list()
# my_list = [] ordered, duplicate allowed, indexed, slicing, mutable
print(my_list)
random.shuffle(my_list) #inplace
print(my_list)



import random

my_list = ['ram', 'shyam', 'hari', 'sita', 'ram', 'Roshan']
# my_list = list()
# my_list = [] ordered, duplicate allowed, indexed, slicing, mutable
print(my_list)
random.shuffle(my_list) #inplace
print(random.sample(my_list,4))
print(my_list)



import random

my_list = ['ram', 'shyam', 'hari', 'sita', 'ram', 'Roshan']
# my_list = list()
# my_list = [] ordered, duplicate allowed, indexed, slicing, mutable
print(my_list)
random.shuffle(my_list) #inplace
print(random.sample(my_list,4))
print(my_list)
print(random.choice(my_list))




import random
import secrets

my_list = ['ram', 'shyam', 'hari', 'sita', 'ram', 'Roshan']
# my_list = list()
# my_list = [] ordered, duplicate allowed, indexed, slicing, mutable
print(my_list)
random.shuffle(my_list) #inplace
print(random.sample(my_list,4))
print(my_list)
print(random.choice(my_list))
print(secrets.choice(my_list))




my_list = ['avengers', 'spiderman', 'starwars', 'batman', 'thor', 'avengers','jari', 'kabaddi']
print(my_list)
# my_list[0] = 'kanchana'
my_list.append('kanchana')
my_list.append('EvilDead')
print(my_list)


my_list = ['avengers', 'spiderman', 'starwars', 'batman', 'thor', 'avengers','jari', 'kabaddi']
print(my_list)
# my_list[0] = 'kanchana'
my_list.append('kanchana')
my_list.insert(1,'Titanic')
my_list.append('EvilDead')
print(my_list)



