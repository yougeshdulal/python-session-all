my_list2 = ['batman', 'Titanic', 'kanchana']
print(my_list1 + my_list2)



my_list1 = []
my_list2 = ['batman', 'Titanic', 'kanchana']
my_list1 = my_list2
my_list2.append("Thor")
print(my_list1)
print(my_list2)



my_list1 = ['test', 'rest']
my_list2 = ['batman', 'Titanic', 'kanchana']
my_list1 = my_list2.copy()
my_list2.append("Thor")
print(my_list1)
print(my_list2)



my_list1 = ['test', 'rest']
my_list2 = ['batman', 'Titanic', 'kanchana']
my_list1.insert(-1,'Hello')
my_list1.insert(-1,'There')
print(my_list1)
print(my_list2)



my_list1 = ['test', 'rest']
