print(my_list2.count('Titanic'))





#quick catchup
my_list1 = [22, 55, 77, 33]
print(my_list1)
#indexed
print(my_list1[0])
print(my_list1[1])
#slicing
print(my_list1[1:3])
#sorting
my_list1.sort()
print(my_list1)
my_list1.sort(reverse=True)
print(my_list1)


my_list1 = [22, 55, 77, 33]
print(my_list1)
#indexed
print(my_list1[0])
print(my_list1[1])
#slicing
print(my_list1[1:3])
#sorting
my_list1.sort()
print(my_list1)
#reverse
my_list1.sort(reverse=True)
print(my_list1)
print(my_list1[::-1])



my_list1 = [22, 55, 77, 33]
print(my_list1)
#indexed
print(my_list1[0])
print(my_list1[1])
#slicing
print(my_list1[1:3])
# #sorting
# my_list1.sort()
# print(my_list1)
# #reverse
# my_list1.sort(reverse=True)
# print(my_list1)
# print(my_list1[::-1])
#add new value
my_list1.append(999)
my_list1.insert(1,9999)
print(my_list1)
#length
print(len(my_list1))
print(max(my_list1))
print(min(my_list1))
print(sum(my_list1))
print(my_list1[5])
