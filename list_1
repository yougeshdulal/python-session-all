onumber = 7
guessnumber = input("Guess the number: ")
if onumber == guessnumber:
    print("Congratulations")
else:
    print("Try Again!")


import random
#pseudo random numbers
random.seed(2)
print(random.random())
print(random.uniform(1,5))
print(random.randint(1,5))

random.seed(2)
print(random.random())
print(random.uniform(1,5))
print(random.randint(1,5))



import secrets

print(secrets.randbelow(5))
print(secrets.randbits(4))
