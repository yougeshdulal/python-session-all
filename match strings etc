name = 'rshanstha'
print(name)
print(name.isdigit())
print(name.isalpha())
print(name.isalnum())
print(name.islower())



name = 'rshanstha'
print(name[0:4])
print(name[::])
print(name[1:])


name = 'rshanstha'
print(name[0:4])
print(name[::])
print(name[0:8:2])  #rshanstha



name = 'rshanstha'
print(name[0:4])
print(name[::])
print(name[0::2])  #rshanstha
print(name[-1])


