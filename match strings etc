while True:
    choice = input("Enter add sub or exit: ")
    match choice:
        case 'add':
            print("This is inside addition")
        case 'sub':
            print("This is sub block")
        case 'exit':
            exit()
        case _:
            print("Invalid")


import random
while True:
    num = random.randint(1,5)
    print(num,end='')









