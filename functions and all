def summation(a, b):
    c = a + b
    print(f"sum is {c}")
    

summation(22,33)


def summation(a, b):
    c = a + b
    return c


res = summation(22,33)
print(f"Sum is {res}")
print(f"Sum is {summation(22, 33)}")




def demo(x):
    a = 333 + x
    return a


res = demo(22)
if res > 100:
    print(">100")
	
	
	
def summation(a, b):
    c = a + b
    return c


res = summation(22,33)
print(f"Sum is {res}")
print(f"Sum is {summation(22, 33)}")

