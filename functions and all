def list_test(some_list):
    # print(some_list)
    some_list.sort(reverse=True)
    return some_list


assert list_test([1,2,3]) == [3,2,1],"Issue is there"
print("Everything is alright")



def list_test(some_list):
    # print(some_list)
    # print(max(some_list))
    max_number = some_list[0]
    for i in some_list:
        if max_number < i:
            max_number = i
    print("Largest number is",max_number)


list_test([1, 2, 3, 7, 99, 5])
# 1 < 2
#     maxnumber = 2
#     2<3?
#         maxnumber = 3
#             3<7?
# maxnumber = 7
# 7<99?
# maxnumber =99
# 99<5?
#     NO


def list_test(some_list):
    # print(some_list)
    # print(max(some_list))
    max_number = some_list[0]
    for i in some_list:
        if max_number < i:
            max_number = i
            print("Largest number is",max_number)


list_test([1, 2, 3, 7, 99, 5])
# 1 < 2
#     maxnumber = 2
#     2<3?
#         maxnumber = 3
#             3<7?
# maxnumber = 7
# 7<99?
